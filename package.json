{
	"name": "Entertain",
	"displayName": "Entertain",
	"description": "Shows the number of selected lines in the status bar",
	"icon": "resources/entertain.png",
	"version": "0.0.3",
	"publisher": "Allent",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/AllentDan/entertain"
	},
	"bugs": {
		"url": "https://github.com/AllentDan/entertain/issues"
	},
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onCommand:extension.formatTable"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.formatTable",
				"title": "Format table to sphinx style"
			},
			{
				"command": "extension.generateTree",
				"title": "Genrate a tree"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder",
					"command": "extension.generateTree",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^17.0.10",
		"@types/vscode": "^1.32.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.4.3"
	}
}
